{
  "background": {
      "color": "#000"
  },
  "interactivity": {
    "events": {
        "onhover": {
          "enable": true,
          "mode": "bubble"
        },
        "resize": true
    },
    "detect_on": "window",
    "modes": {
      "bubble": {
        "distance": 300,
        "size": 10,
        "duration": 2,
        "speed": 3
      }
    }
  },
  "particles": {
      "color": {
          "animation": {
              "enable": true,
              "sync": false,
              "speed": 50
          },
          "value": "#bbb"
      },
      "links": {
          "color": "random",
          "enable": true
      },
      "collisions": {
          "enable": true
      },
      "move": {
          "enable": true,
          "speed": 1
      },
      "number": {
          "density": {
              "enable": true
          },
          "value": 200
      },
      "opacity": {
          "animation": {
              "enable": true,
              "minimumValue": 0.3,
              "speed": 0.5
          },
          "value": 0.8,
          "random": {
              "enable": true,
              "minimumValue": 0.3
          }
      },
      "size": {
          "animation": {
              "enable": false,
              "minimumValue": 1,
              "speed": 3
          },
          "value": 6,
          "random": {
              "enable": true,
              "minimumValue": 1
          }
      }
  },
  "emitters": []
}
